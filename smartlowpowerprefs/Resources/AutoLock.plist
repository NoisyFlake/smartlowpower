<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>

		<dict>
			<key>cell</key> 						<string>PSGroupCell</string>
			<key>footerText</key>  			<string>Turn off Auto-Lock in Low Power Mode or when the device is charging. This will not prevent your screen from dimming.</string>
		</dict>
		<dict>
			<key>cell</key>							<string>PSSwitchCell</string>
			<key>default</key>					<false/>
			<key>defaults</key>					<string>com.noisyflake.smartlowpowerprefs</string>
			<key>key</key>							<string>alLPM</string>
			<key>label</key>						<string>Disable in Low Power Mode</string>
			<key>PostNotification</key> <string>com.noisyflake.smartlowpowerprefs/prefsupdated</string>
		</dict>
		<dict>
			<key>cell</key>							<string>PSSwitchCell</string>
			<key>default</key>					<false/>
			<key>defaults</key>					<string>com.noisyflake.smartlowpowerprefs</string>
			<key>key</key>							<string>alCharging</string>
			<key>label</key>						<string>Disable when Charging</string>
			<key>PostNotification</key> <string>com.noisyflake.smartlowpowerprefs/prefsupdated</string>
		</dict>

	</array>
	<key>title</key>
	<string>Auto-Lock</string>
</dict>
</plist>
